<tr id=<%= "cronograma_#{cronograma.id}" %>>
   <td><%= cronograma.fecha %></td>
   <td><%= cronograma.lugar_salida %></td>
   <td><%= cronograma.ficha_grupo %></td>
   <td><%= cronograma.tipo_salida_id %></td>
   <td><%= cronograma.programa %></td>
   <td><%= cronograma.descripcion %></td>
   <td><%= link_to 'Ver', cronograma, :class => 'btn btn-success btn-xs' %></td>
   <td><%= link_to 'Editar', edit_cronograma_path(cronograma),:class=>'btn btn-warning btn-xs', remote:true %></td>
   <td><%= link_to 'Eliminar', cronograma, method: :delete, data: { confirm: 'Esta Seguro?' }, :class=>'btn btn-danger btn-xs',remote:true %></td>
</tr>

