<div class="contenido ">
  <div class="img1"></div>
  <div class="img2"></div>
    <ul class="nav nav-tabs" style="margin-bottom: 15px;">

    </ul>
  <div id="myTabContent" class="tab-content">
    <div class="tab-pane fade active in cont1" id="inicio">

      <p id="notice">
        <%= notice %>
      </p>

      <p>
        <strong>Titulo</strong>
        <%= @event.title %>
      </p>

      <p>
        <strong>Descripcion:</strong>
        <%= @event.description %>
      </p>

      <p>
        <strong>Empieza en:</strong>
        <%= @event.start %>
      </p>

      <p>
        <strong>Termina en:</strong>
        <%= @event.end %>
      </p>

     
      <%=link_to 'Editar', edit_event_path(@event)%>
    
     
     <%=link_to 'Eliminar', event_path(@event), method: :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => "Esta Seguro?")) } %>
     
      <%= link_to 'Atras', events_path %>
    </div>
  </div>
</div>